<br> <br>

<h1> Users </h1>
<% @users.each do |user| %>
	<p>
		<%= user.name %>
	</p>
<% end %> <br> <br>


<h1> Reminders </h1>
<% @reminders.each do |reminder| %>
	<p>
		Remind - <%= reminder.user.name %> -
		to be in touch with - <%= reminder.content %> -
		on - <%= reminder.date %>
	</p>
<% end %>


